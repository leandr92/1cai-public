{
  "dashboard": {
    "title": "1C AI Stack - System Overview",
    "uid": "system_overview",
    "schemaVersion": 39,
    "version": 2,
    "tags": ["system", "overview"],
    "timezone": "browser",
    "editable": true,
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m"],
      "time_options": ["1h", "6h", "12h", "24h", "7d", "30d"]
    },
    "templating": {
      "list": [
        {
          "name": "datasource",
          "type": "datasource",
          "label": "Prometheus",
          "query": "prometheus",
          "refresh": 1,
          "current": {
            "text": "Prometheus",
            "value": "Prometheus"
          }
        },
        {
          "name": "job",
          "type": "query",
          "label": "Job",
          "datasource": "$datasource",
          "definition": "label_values(up, job)",
          "refresh": 2,
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "endpoint",
          "type": "query",
          "label": "Endpoint",
          "datasource": "$datasource",
          "definition": "label_values(http_requests_total{job=~\"$job\"}, endpoint)",
          "refresh": 2,
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "agent",
          "type": "query",
          "label": "Agent",
          "datasource": "$datasource",
          "definition": "label_values(ai_queries_total, agent_type)",
          "refresh": 2,
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        },
        {
          "name": "cache_layer",
          "type": "query",
          "label": "Cache Layer",
          "datasource": "$datasource",
          "definition": "label_values(cache_operations_total, layer)",
          "refresh": 2,
          "multi": true,
          "includeAll": true,
          "current": {
            "text": "All",
            "value": "$__all"
          }
        }
      ]
    },
    "panels": [
      {
        "type": "row",
        "title": "Трафик",
        "id": 1,
        "gridPos": {"x": 0, "y": 0, "w": 24, "h": 1}
      },
      {
        "type": "stat",
        "title": "Request Rate",
        "id": 2,
        "datasource": "$datasource",
        "gridPos": {"x": 0, "y": 1, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=~\"$job\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 5},
                {"color": "green", "value": 15}
              ]
            }
          }
        }
      },
      {
        "type": "stat",
        "title": "Success Rate %",
        "id": 3,
        "datasource": "$datasource",
        "gridPos": {"x": 6, "y": 1, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{status_code=~\"2..\", job=~\"$job\", endpoint=~\"$endpoint\"}[$__rate_interval])) / clamp_min(sum(rate(http_requests_total{job=~\"$job\", endpoint=~\"$endpoint\"}[$__rate_interval])), 0.001) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 95},
                {"color": "green", "value": 98}
              ]
            }
          }
        }
      },
      {
        "type": "stat",
        "title": "Active Users",
        "id": 4,
        "datasource": "$datasource",
        "gridPos": {"x": 12, "y": 1, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "active_users",
            "refId": "A"
          }
        ]
      },
      {
        "type": "stat",
        "title": "Active Tenants",
        "id": 5,
        "datasource": "$datasource",
        "gridPos": {"x": 18, "y": 1, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(active_tenants{status=\"active\"})",
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "Requests per Method",
        "id": 6,
        "datasource": "$datasource",
        "gridPos": {"x": 0, "y": 5, "w": 12, "h": 8},
        "fieldConfig": {
          "defaults": {"unit": "reqps"}
        },
        "targets": [
          {
            "expr": "sum by (method) (rate(http_requests_total{job=~\"$job\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
            "legendFormat": "{{method}}",
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "HTTP latency p95",
        "id": 7,
        "datasource": "$datasource",
        "gridPos": {"x": 12, "y": 5, "w": 12, "h": 8},
        "fieldConfig": {
          "defaults": {"unit": "s"}
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (le, endpoint) (rate(http_request_duration_seconds_bucket{job=~\"$job\", endpoint=~\"$endpoint\"}[$__rate_interval])))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "HTTP 5xx rate",
        "id": 8,
        "datasource": "$datasource",
        "gridPos": {"x": 0, "y": 13, "w": 12, "h": 8},
        "fieldConfig": {
          "defaults": {"unit": "reqps"}
        },
        "targets": [
          {
            "expr": "sum by (endpoint) (rate(http_requests_total{status_code=~\"5..\", job=~\"$job\", endpoint=~\"$endpoint\"}[$__rate_interval]))",
            "legendFormat": "{{endpoint}}",
            "refId": "A"
          }
        ]
      },
      {
        "type": "row",
        "title": "AI сервисы",
        "id": 9,
        "gridPos": {"x": 0, "y": 21, "w": 24, "h": 1}
      },
      {
        "type": "timeseries",
        "title": "AI Queries",
        "id": 10,
        "datasource": "$datasource",
        "gridPos": {"x": 0, "y": 22, "w": 12, "h": 8},
        "fieldConfig": {
          "defaults": {"unit": "reqps"}
        },
        "targets": [
          {
            "expr": "sum by (agent_type) (rate(ai_queries_total{agent_type=~\"$agent\"}[$__rate_interval]))",
            "legendFormat": "{{agent_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "AI latency p95",
        "id": 11,
        "datasource": "$datasource",
        "gridPos": {"x": 12, "y": 22, "w": 12, "h": 8},
        "fieldConfig": {
          "defaults": {"unit": "s"}
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum by (le, agent_type) (rate(ai_response_duration_seconds_bucket{agent_type=~\"$agent\"}[$__rate_interval])))",
            "legendFormat": "{{agent_type}}",
            "refId": "A"
          }
        ]
      },
      {
        "type": "row",
        "title": "Cache и ресурсы",
        "id": 12,
        "gridPos": {"x": 0, "y": 30, "w": 24, "h": 1}
      },
      {
        "type": "stat",
        "title": "Cache Hit %",
        "id": 13,
        "datasource": "$datasource",
        "gridPos": {"x": 0, "y": 31, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(rate(cache_operations_total{operation=\"get\", status=\"hit\", layer=~\"$cache_layer\"}[$__rate_interval])) / clamp_min(sum(rate(cache_operations_total{operation=\"get\", layer=~\"$cache_layer\"}[$__rate_interval])), 0.001) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 80},
                {"color": "green", "value": 90}
              ]
            }
          }
        }
      },
      {
        "type": "stat",
        "title": "DB pool size",
        "id": 14,
        "datasource": "$datasource",
        "gridPos": {"x": 6, "y": 31, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(db_pool_size)",
            "refId": "A"
          }
        ]
      },
      {
        "type": "stat",
        "title": "DB pool available",
        "id": 15,
        "datasource": "$datasource",
        "gridPos": {"x": 12, "y": 31, "w": 6, "h": 4},
        "targets": [
          {
            "expr": "sum(db_pool_available_connections)",
            "refId": "A"
          }
        ]
      },
      {
        "type": "timeseries",
        "title": "System CPU usage",
        "id": 16,
        "datasource": "$datasource",
        "gridPos": {"x": 18, "y": 31, "w": 6, "h": 8},
        "fieldConfig": {
          "defaults": {"unit": "percent"}
        },
        "targets": [
          {
            "expr": "system_cpu_usage_percent",
            "legendFormat": "system",
            "refId": "A"
          }
        ]
      }
    ]
  }
}


